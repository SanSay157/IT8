		</ds:prop>
		<ds:prop n="DateRatio" d="Масштаб" vt="object" ot="DateRatio" cp="scalar">
			<i:object-dropdown empty-value-text="(Укажите масштаб времени)" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0" />
		</ds:prop>
