							<i:param n="RefreshFlags">TRM_CHILDS+TRM_NODE+TRM_PARENTNODE</i:param>
						</i:params>
					</i:menu-item>
					<i:menu-item action="DoDelete" hotkey="VK_DEL" t="Удалить" separator-after="1" separator-before="1">
