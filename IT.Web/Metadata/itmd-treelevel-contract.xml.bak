						<i:param n="RefreshFlags">TRM_CHILDS+TRM_NODE</i:param>
					</i:params>
				</i:menu-item>
				<i:menu-item action="DoCreate" t="Создать расходный лимит по проекту">
					<i:params>
						<i:param n="ObjectType">OutLimit</i:param>
						<i:param n="URLPARAMS">.Contract=@@ContractID</i:param>
						<i:param n="RefreshFlags">TRM_CHILDS+TRM_NODE</i:param>
