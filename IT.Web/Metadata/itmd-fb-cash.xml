		<ds:prop n="Parent" d="Вышестоящий" vt="object" ot="AOReason" cp="scalar" maybenull="1">
			<i:object-presentation xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
				<i:value>item.Name</i:value>
				<i:prop-menu>
