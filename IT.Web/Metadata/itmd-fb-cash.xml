	<ds:type n="EmpMoneyMove" d="Передача ДС" tp="persistent">
		<ds:prop n="From" d="Передал" vt="object" ot="Employee" cp="scalar">
			<i:object-presentation off-create="1" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
				<i:value>it_FormatName(item.LastName, item.FirstName, item.PhoneExt)</i:value>
				<i:prop-menu>
					<i:menu ref="std-object-presentation" />
				</i:prop-menu>
			</i:object-presentation>
		</ds:prop>
		<ds:prop n="To" d="Получил" vt="object" ot="Employee" cp="scalar">
			<i:object-presentation off-create="1" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
				<i:value>it_FormatName(item.LastName, item.FirstName, item.PhoneExt)</i:value>
				<i:prop-menu>
					<i:menu ref="std-object-presentation" />
				</i:prop-menu>
			</i:object-presentation>
		</ds:prop>
		<ds:prop n="Date" d="Дата" vt="date" cp="scalar" />
		<ds:prop n="Sum" d="Сумма" vt="fixed" cp="scalar" />
		<ds:prop n="Rem" d="Примечание" vt="string" cp="scalar" maybenull="1">
			<ds:max>1024</ds:max>
		</ds:prop>
		<i:editor n="editor" width="500" height="350" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<i:client-script>it-format-value.vbs</i:client-script>
			<i:page n="Main" t=" ">x-editor2.xsl</i:page>
			<i:menu ref="std-editor-menu" />
		</i:editor>
		<i:objects-list t="Передача ДС" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
				SELECT TOP @@MAXROWS 
					t.[ObjectID],
					t.[Date],
					dbo.GetSumString(t.[Sum], NULL),
          dbo.GetEmployeeString(t.[From]),
          dbo.GetEmployeeString(t.[To]),
					t.[Rem]
				FROM [dbo].[EmpMoneyMove] t
				WHERE_CLAUSE
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="DateFrom" vt="dateTime">
						<ds:param-selector op="not-is-null">
              <![CDATA[ 
							t.Date >= dbo.DATETRIM(:DateFrom) 
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="DateTo" vt="dateTime">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							t.Date < DATEADD(d, 1, dbo.DATETRIM(:DateTo))
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="From" vt="uuid">
						<ds:param-selector op="not-is-null">
              <![CDATA[
						  t.[From] = :From
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="To" vt="uuid">
						<ds:param-selector op="not-is-null">
              <![CDATA[
						  t.[To] = :To
						]]>
            </ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
			<i:filter-as-editor ot="FilterEmpMoneyMove" height="60" />
			<i:list-menu>
				<i:menu ref="std-list" />
			</i:list-menu>
			<i:column t="Дата" vt="date" align="center" width="200" />
			<i:column t="Сумма" align="right" width="200" />
			<i:column t="Передал" width="300" />
			<i:column t="Получил" width="300" />
			<i:column t="Примечание" width="400" />
		</i:objects-list>
	</ds:type>
