			<!--i:client-script>it-editor-security.vbs</i:client-script-->
			<i:page t="Main">x-editor2.xsl?@Type</i:page>
			<i:menu ref="std-editor-menu" />
		</i:editor>
		<i:objects-list t="Инцидент" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
				SELECT TOP @@MAXROWS 
					t.[ObjectID],
					t.[Number],
					t.[Name],
					t.[Priority],
					t.[InputDate],
					t.[DeadLine],
					t.[State] Icon_Selector
				FROM [dbo].[Incident] t WITH (NOLOCK)
				WHERE_CLAUSE
				]]>
        </ds:cmd-text>
			</ds:data-source>
			<i:list-menu>
				<i:menu ref="std-list" />
			</i:list-menu>
			<i:column t="Номер" vt="i4" width="100" />
			<i:column t="Наименование" width="100" />
			<i:column t="Приоритет" vt="i2" width="100" />
			<i:column t="Дата регистрации" vt="dateTime" width="100" />
			<i:column t="Крайний срок" vt="date" width="100" />
		</i:objects-list>
		<!--
			Список инцидентов текущего пользователя
		-->
		<i:objects-list t="Мои инциденты" n="CurrentTaskList" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<i:client-script>s-list-CurrentTaskList.vbs</i:client-script>
			<i:client-script>x-create-outlook-letter.vbs</i:client-script>
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
					SELECT TOP @@MAXROWS
						i.ObjectID AS IncidentID, 
						i.Number AS IncidentNumber,
						i.Name AS IncidentName,
						role.Name AS IncidentRoleName,
						st.Name AS IncidentStateName,
						IsNull(Convert(varchar(10), i.DeadLine, 104), '') AS DeadLine,
						CASE i.Priority 
							WHEN 1 THEN 'Высокий'
							WHEN 2 THEN 'Средний'
							WHEN 3 THEN 'Низкий'
						END AS IncidentPriorityName,
						it.Name AS IncidentTypeName, 
						dbo.GetFolderPath(i.Folder, 0) AS DisplayFolderPath,
						dbo.GetFolderPath(i.Folder, 1) AS FolderPath,
						convert(varchar(10), i.InputDate, 104) AS InputDateStr,
						i.InputDate,
						t.ObjectID AS OwnTaskID,
						i.Priority As IncidentPriority,
						convert( float, convert(DateTime,'December 31, 9999')) -  convert(float,isnull(i.deadline,'December 31, 9999')) +1 DeadlineSort,
						InPlan = #InPlanColumnExpr#,
						i.State AS ICON_SELECTOR
					FROM
						dbo.Incident i WITH (NOLOCK) 
							JOIN dbo.IncidentState st WITH (NOLOCK) on i.State = st.ObjectID AND st.[Category] IN (1,2) /* Ограничение по открытому состоянию */
						JOIN  dbo.Task t WITH (NOLOCK) on i.ObjectID = t.Incident
							JOIN dbo.UserRoleInIncident role WITH (NOLOCK) on t.Role = role.ObjectID
						JOIN dbo.Folder f WITH (NOLOCK) on i.Folder = f.ObjectID AND f.State IN (1,2)
						JOIN dbo.IncidentType it WITH (NOLOCK) on i.Type = it.ObjectID
					WHERE
						t.Worker = @CurrentEmployeeID AND t.IsFrozen=0
						#OnlyActiveTasksCond#
						AND SEARCH_CONDITIONS
					ORDER BY 
						InPlan DESC, 
						DeadLineSort Desc, 
						i.Priority, 
						i.InputDate, 
						it.Name
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="CurrentEmployeeID" vt="uuid" />
					<ds:param n="IncidentID" vt="uuid">
						<ds:param-selector op="not-is-null">i.ObjectID = @IncidentID</ds:param-selector>
					</ds:param>
					<ds:param n="RestrictedList" vt="boolean">
						<ds:switch macro="#OnlyActiveTasksCond#" explicit="1">
							<!-- Только если есть переходы для роли текущего юзера из текущего состояния 
								И для роли не отключено отображение в сокращенном списке -->
							<ds:param-selector op="equal" value="1">
                <![CDATA[
									AND EXISTS(
										SELECT 1 FROM dbo.Transition tr WITH (NOLOCK) 
										WHERE tr.Role = role.ObjectID AND tr.[From] = st.ObjectID
									) 
									AND role.ShowInShortTaskList = 1
								]]>
              </ds:param-selector>
						</ds:switch>
						<ds:switch macro="#InPlanColumnExpr#" explicit="1">
							<ds:param-selector op="equal" value="0">
                <![CDATA[
									CASE 
										WHEN 
											/* существует переходы для роли из текущего состояния */
											EXISTS(
												SELECT 1 FROM dbo.Transition tr WITH (NOLOCK) 
												WHERE tr.Role = role.ObjectID AND tr.[From] = st.ObjectID
											)
										THEN 1 
										ELSE 0 
									END
								]]>
              </ds:param-selector>
							<ds:param-selector op="not-equal" value="0">1</ds:param-selector>
							<ds:param-selector op="is-null" value="0">1</ds:param-selector>
						</ds:switch>
					</ds:param>
					<ds:param n="IncidentTypes" array="1" vt="uuid">
						<ds:param-selector op="in">it.ObjectID</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">i.Name LIKE @IncidentName</ds:param-selector>
					</ds:param>
					<ds:param n="FolderName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							EXISTS (SELECT 1 FROM Folder f_i  WITH (NOLOCK) 
								WHERE f_i.LIndex <= f.LIndex AND f_i.RIndex >= f.RIndex AND f_i.Customer = f.Customer AND f_i.Name LIKE @FolderName)
							]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="IncidentPriority" vt="i2">
						<ds:param-selector op="not-is-null">i.Priority = @IncidentPriority</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentStateCategory" vt="i2">
						<ds:param-selector op="not-is-null">st.[Category] = @IncidentStateCategory</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentsWithDeadline" vt="boolean">
						<ds:param-selector op="equal" value="1">i.Deadline IS NOT NULL</ds:param-selector>
					</ds:param>
					<ds:param n="DeadlineInNextDays" vt="i4">
						<!-- TODO: Это tableScan, надо переделать  -->
						<ds:param-selector op="not-is-null">datediff(d, getDate(), i.Deadline) BETWEEN 0 AND @DeadlineInNextDays</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentsWithExpiredDeadline" vt="boolean">
						<!-- Дата дедлайна меньше текущей даты -->
						<ds:param-selector op="equal" value="1"><![CDATA[ i.Deadline IS NOT NULL AND i.Deadline < getDate() ]]></ds:param-selector>
					</ds:param>
