		<!--

		Списанное время текущего сотрудника -->
		<i:objects-list t="Списание времени сотрудником" n="TimeLossSearchingList" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0" off-load="1">
			<!-- <ds:data-source ref="TimeLossSearchingList" with-params="EmployeeIDParamName=CurrentEmployeeID"/> -->
			<ds:data-source n="TimeLossSearchingList" xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
					SELECT  
						tl.ObjectID,
            tl.Worker,
            tl.Folder
          INTO #Data
					FROM 
            dbo.TimeLoss tl (NOLOCK)
						LEFT JOIN dbo.Folder f (NOLOCK) ON 
              tl.Folder = f.ObjectID
					WHERE_CLAUSE
          
          IF :OnlyOwnTimeLoss = 0
          BEGIN
            SELECT 
	            pp.ObjectID,
	            pp.Folder,
	            pp.Privileges
            INTO #ProjectParticipants
            FROM 
	            ProjectParticipant pp (NOLOCK) 
            WHERE 
	            pp.Employee = @CurrentEmployeeID 
          
            DELETE t
            FROM 
              #Data t
              JOIN (SELECT ObjectID FROM #Data WHERE Worker <> @CurrentEmployeeID) tt ON
                t.ObjectID = tt.ObjectID
              LEFT JOIN (
               SELECT DISTINCT
	              ff.ObjectID
              FROM
	              Folder f (NOLOCK)
	              JOIN (
		              SELECT 
			              pp.Folder 
		              FROM 
			              #ProjectParticipants pp 
		              WHERE pp.Privileges & 4 > 0
		              UNION
		              SELECT 
			              pp.Folder 
		              FROM 
			              #ProjectParticipants pp 
			              JOIN ProjectParticipant_Roles pp_r (NOLOCK) ON
				              pp.ObjectID = pp_r.ObjectID
			              JOIN (SELECT ObjectID FROM UserRoleInProject (NOLOCK) WHERE Privileges & 4 > 0) urip ON
				              urip.ObjectID = pp_r.Value 
	              ) pp ON
		              pp.Folder = f.ObjectID
	              JOIN Folder ff (NOLOCK) ON
		              f.Customer = ff.Customer
		              AND f.LIndex <= ff.LIndex
		              AND ff.RIndex <= f.RIndex
              ) f ON
                t.Folder = f.ObjectID
            WHERE
              f.ObjectID IS NULL
            		
            DROP TABLE #ProjectParticipants
          END
          
          SELECT TOP @@MAXROWS 
						tl.ObjectID,
						emp.LastName + ' ' + emp.FirstName,
						CONVERT(varchar(10), tl.LossFixed, 104) AS LossFixed,
						CASE 
							WHEN tl.LostTime/60 > 0 THEN 
								convert(varchar(4), tl.LostTime/60 ) + ' ч. ' + convert(varchar(4), tl.LostTime%60 ) + ' м.' 
							ELSE convert(varchar(4), tl.LostTime ) + ' м.'  
						END,
						tlc.Name,
						IsNull( dbo.GetFolderPath(tl.Folder, 0), '- не указан -'),
						tl.ExternalID,
						tl.Descr,
						tl.LossFixed AS LossFixed4Order
          FROM 
            #Data t
            JOIN TimeLoss tl (NOLOCK) ON
              tl.ObjectID = t.ObjectID
						JOIN TimeLossCause tlc (NOLOCK) ON tl.Cause = tlc.ObjectID
						JOIN Employee emp (NOLOCK) ON tl.Worker = emp.ObjectID
					ORDER BY tl.LossFixed DESC, tl.LostTime DESC
          
          DROP TABLE #Data
				  ]]>
        </ds:cmd-text>
				<ds:params>
					<ds:params-processor class-name="Croc.IncidentTracker.DataSources.TimeLossSearchingListParamsProcessor,Croc.IncidentTracker.Commands" />
					<ds:param n="CurrentEmployeeID" vt="uuid" required="1" />
					<ds:param n="OnlyOwnTimeLoss" vt="boolean">
						<ds:param-selector op="equal" value="1">tl.Worker = @CurrentEmployeeID</ds:param-selector>
						<ds:param-selector op="equal" value="0">tl.Worker = @CurrentEmployeeID OR tl.Folder IS NOT NULL</ds:param-selector>
					</ds:param>
					<ds:param n="Employees" vt="uuid" array="1">
						<ds:param-selector op="in">tl.Worker</ds:param-selector>
					</ds:param>
					<ds:param n="LossFixedStart" vt="date">
						<ds:param-selector op="not-is-null"><![CDATA[ tl.LossFixed >= @LossFixedStart ]]></ds:param-selector>
					</ds:param>
					<ds:param n="LossFixedEnd" vt="date">
						<ds:param-selector op="not-is-null"><![CDATA[ tl.LossFixed < @LossFixedEnd + 1 ]]></ds:param-selector>
					</ds:param>
					<ds:param n="Causes" array="1" vt="uuid">
						<ds:param-selector op="in">tl.Cause</ds:param-selector>
					</ds:param>
					<ds:param n="RecursiveFolderSearch" vt="boolean">
						<ds:switch>
							<ds:param-selector op="equal" value="0">tl.Folder IN (#FolderIDs#)</ds:param-selector>
							<ds:param-selector op="equal" value="1">
                <![CDATA[
								tl.Folder IN (
									SELECT f.ObjectID FROM Folder f WITH (NOLOCK)
										JOIN Folder f_s WITH (NOLOCK) ON f.LIndex >= f_s.LIndex AND f.RIndex <= f_s.RIndex AND f_s.Customer = f.Customer
									WHERE f_s.ObjectID IN (#FolderIDs#)
									)
							]]>
              </ds:param-selector>
						</ds:switch>
					</ds:param>
					<ds:param n="Folders" array="1" vt="uuid">
						<ds:array-param-processor macro="#FolderIDs#" />
					</ds:param>
					<ds:param n="ExternalID" vt="string" use-wildcard="implicit-on-end">
						<ds:param-selector op="not-is-null">tl.ExternalID LIKE :ExternalID</ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
			<i:filter-as-editor ot="FilterTimeLossSearchingList" height="235" />
			<i:list-menu>
				<i:menu ref="std-list" />
			</i:list-menu>
			<i:column t="Сотрудник" width="150" />
			<i:column t="Дата списания" width="100" order-by="LossFixed4Order" />
			<i:column t="Количество времени" width="100" />
			<i:column t="Причина списания" width="250" />
			<i:column t="Проект" width="300" />
