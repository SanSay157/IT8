		<!--
			Интеграция с Microsoft Project 2003
		-->
		<i:objects-list t="Интеграция с Microsoft Project: список заданий" n="MsProjectIntegration_Tasks" maxrows="1000000" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
					SELECT
						t.ObjectID,
						t.LeftTime, 
						t.PlannedTime,
						(select sum( ts.Spent) 
							from dbo.TimeSpent ts WITH (NOLOCK) 
							where ts.Task = t.ObjectID) as TimeSpent,
						(select min( ts.RegDate) 
							from dbo.TimeSpent ts WITH (NOLOCK) 
							where ts.Task = t.ObjectID) as ActualStart,
						(select max( ts.RegDate)
							from dbo.TimeSpent ts WITH (NOLOCK) 
							where ts.Task = t.ObjectID) as ActualFinish,
						emp.LastName + emp.FirstName AS EmployeeName,
						emp.EMail,
						su.Login,
						role.Name
					FROM
						dbo.Task t WITH (NOLOCK)
						JOIN dbo.Employee emp WITH (NOLOCK) ON t.Worker = emp.ObjectID
							LEFT JOIN dbo.SystemUser su WITH (NOLOCK) on emp.SystemUser = su.ObjectID
						JOIN dbo.UserRoleInIncident role WITH (NOLOCK) on t.Role = role.ObjectID
					WHERE t.Incident = :IncidentID
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="IncidentID" vt="uuid" />
				</ds:params>
			</ds:data-source>
			<i:column t="Осталось минут" width="90" vt="i4" />
			<i:column t="Запланировано минут" width="90" vt="i4" />
			<i:column t="Потрачено минут" width="90" vt="i4" />
			<i:column t="Дата начала" width="90" vt="dateTime" />
			<i:column t="Дата завершения (последнего списания)" width="90" vt="dateTime" />
			<i:column t="ФИО исполнителя" width="200" />
			<i:column t="EMail" width="150" />
			<i:column t="Login" width="200" />
			<i:column t="Роль в задании" width="200" />
		</i:objects-list>
	</ds:type>
