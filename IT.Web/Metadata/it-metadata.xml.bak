		-->
		<i:objects-list t="Интеграция с Microsoft Project: список инцидентов" n="MsProjectIntegration_Incidents" maxrows="1000000" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
				SELECT
					i.ObjectID,
					i.Name,
					i.Number,
					i.Deadline,
					CASE i.Priority
						when 0 then 800
						when 1 then 500
						when 2 then 300
					END,
					CASE st.Category
						WHEN 3 then 1	/* Выполнен */
						WHEN 4 then 1	/* Заморожен */
						WHEN 5 then 1	/* Отклонен */
						ELSE 0
					END AS IsClosedState,
					itype.[Name],
					i.[Descr]
				FROM 
					dbo.Incident i WITH (NOLOCK)
						JOIN dbo.IncidentType itype WITH (NOLOCK) ON i.Type = itype.ObjectID
						JOIN dbo.IncidentState st WITH (NOLOCK) ON i.State = st.ObjectID
				WHERE_CLAUSE
				ORDER BY 
					i.deadline Desc, 
					Priority Desc
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="FolderID" vt="uuid">
						<ds:param-selector op="not-is-null"><![CDATA[ i.Folder = :FolderID ]]></ds:param-selector>
					</ds:param>
					<ds:param n="IncidentID" vt="uuid">
						<ds:param-selector op="not-is-null"><![CDATA[ i.ObjectID = :IncidentID ]]></ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
