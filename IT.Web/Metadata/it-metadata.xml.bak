			<i:column t="Комментарий" width="300" />
			<i:column t=" " n="LossFixed4Order" vt="dateTime" width="0" />
		</i:objects-list>
		<!--

		Списанное время сотрудников (всех / указанных) -->
		<i:objects-list t="Списание времени сотрудников" n="TimeLossSearchingListAdm" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0" off-load="1">
