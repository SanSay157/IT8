		-->
		<i:objects-list t="Инциденты" n="IncidentSearchingList" off-load="1" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<i:client-script>s-list-CurrentTaskList.vbs</i:client-script>
			<i:client-script>s-list-IncidentSearchingList.vbs</i:client-script>
			<i:client-script>x-create-outlook-letter.vbs</i:client-script>
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
					SET NOCOUNT ON
					SET ROWCOUNT @@MAXROWS
					SELECT TOP @@MAXROWS
						i.ObjectID AS IncidentID, 
						i.Number AS IncidentNumber,
						i.Name AS IncidentName,
						st.Name AS IncidentStateName,
						IsNull(Convert(varchar(10), i.DeadLine, 104), '') AS DeadLine,
						CASE i.Priority 
							WHEN 1 THEN 'Высокий'
							WHEN 2 THEN 'Средний'
							WHEN 3 THEN 'Низкий'
						END AS IncidentPriorityName,
						it.Name AS IncidentTypeName, 
						dbo.GetFolderPath(i.Folder, 0) AS DisplayFolderPath,
						dbo.GetFolderPath(i.Folder, 1) AS FolderPath,
						convert(varchar(10), i.InputDate, 104) AS InputDateStr,
						i.InputDate,
						i.Priority AS IncidentPriority,
						i.State AS ICON_SELECTOR
					FROM
						dbo.Incident i WITH (NOLOCK)
						JOIN dbo.Employee e WITH (NOLOCK) on e.SystemUser = i.Initiator
						JOIN dbo.Folder f WITH (NOLOCK) on i.Folder = f.ObjectID
						JOIN dbo.IncidentState st WITH (NOLOCK) on i.State = st.ObjectID
						JOIN dbo.IncidentType it WITH (NOLOCK) on i.Type = it.ObjectID
					WHERE (SEARCH_CONDITIONS OR #FindIncidentByID#)
					ORDER BY 
						i.Number
					SET ROWCOUNT 0
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:params-processor class-name="Croc.IncidentTracker.DataSources.IncidentSearchingListParamsProcessor,Croc.IncidentTracker.Commands" />
					<ds:param n="IncidentID" vt="uuid">
						<ds:switch macro="#FindIncidentByID#">
							<ds:param-selector op="not-is-null">i.ObjectID = @IncidentID</ds:param-selector>
							<ds:param-selector op="is-null">1=0</ds:param-selector>
						</ds:switch>
						<ds:switch>
							<ds:param-selector op="not-is-null">1=0</ds:param-selector>
							<ds:param-selector op="is-null">1=1</ds:param-selector>
						</ds:switch>
					</ds:param>
					<ds:param n="IncidentNumber" vt="i4">
						<ds:param-selector op="not-is-null">i.Number = @IncidentNumber</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">i.Name LIKE @IncidentName</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentPriority" vt="i2">
						<ds:param-selector op="not-is-null">i.Priority = @IncidentPriority</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentStateCategoryFlags" vt="i2">
						<ds:param-selector op="not-is-null"><![CDATA[POWER(2, st.[Category] - 1) & @IncidentStateCategoryFlags > 0 OR @IncidentStateCategoryFlags = 0]]></ds:param-selector>
					</ds:param>
					<ds:param n="IncidentTypes" array="1" vt="uuid">
						<ds:param-selector op="in">it.ObjectID</ds:param-selector>
					</ds:param>
					<ds:param n="IncidentsWithDeadline" vt="boolean">
						<ds:param-selector op="equal" value="1">i.Deadline IS NOT NULL</ds:param-selector>
					</ds:param>
					<ds:param n="DeadlineDateBegin" vt="date">
						<ds:param-selector op="not-is-null"><![CDATA[ ((0!=IsNull(@IncidentsWithExpiredDeadline,0)) OR (i.Deadline >= @DeadlineDateBegin ))]]></ds:param-selector>
					</ds:param>
					<ds:param n="DeadlineDateEnd" vt="date">
						<ds:param-selector op="not-is-null"><![CDATA[ ((0!=IsNull(@IncidentsWithExpiredDeadline,0)) OR (i.Deadline < @DeadlineDateEnd + 1))]]></ds:param-selector>
					</ds:param>
					<ds:param n="IncidentsWithExpiredDeadline" vt="boolean">
						<!-- Дата дедлайна меньше даты закрытия инцидента или текущей даты, если инцидент еще не закрыт -->
						<ds:param-selector op="equal" value="1">
              <![CDATA[ i.Deadline IS NOT NULL AND (
							i.Deadline < getDate() AND st.Category IN (1,2) OR 
							NOT st.Category IN (1,2) AND i.DeadLine < (
								SELECT Min(ChangeDate) FROM IncidentStateHistory i_h  WITH (NOLOCK) 
								JOIN IncidentState i_st  WITH (NOLOCK) ON i_h.State = i_st.ObjectID
								WHERE Incident = i.ObjectID AND NOT i_st.Category IN (1,2) 
							) )
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="RecursiveFolderSearch" vt="boolean">
						<ds:switch>
							<ds:param-selector op="equal" value="0">i.Folder IN (#FolderIDs#)</ds:param-selector>
							<ds:param-selector op="equal" value="1">
                <![CDATA[
								i.Folder IN (
									SELECT f.ObjectID FROM Folder f  WITH (NOLOCK) 
										JOIN Folder f_s  WITH (NOLOCK) ON f.LIndex >= f_s.LIndex AND f.RIndex <= f_s.RIndex AND f_s.Customer = f.Customer
									WHERE f_s.ObjectID IN (#FolderIDs#)
									)
							]]>
              </ds:param-selector>
						</ds:switch>
					</ds:param>
					<ds:param n="ExceptParticipants" vt="boolean">
						<ds:switch>
							<ds:param-selector op="equal" value="0">
                <![CDATA[
                                         EXISTS (
                                         SELECT 1 FROM Task t WITH (NOLOCK)
                                         WHERE t.Incident = i.ObjectID AND t.Worker IN (#Participants#)
                                         )
                            ]]>
              </ds:param-selector>
							<ds:param-selector op="equal" value="1">
                <![CDATA[
                                       NOT EXISTS(
                                          SELECT TOP 1 1 FROM dbo.Transition tr WITH (NOLOCK)
                                          INNER HASH JOIN dbo.Task t1 WITH (NOLOCK) ON t1.[Role] = tr.[Role]
                                          WHERE (tr.[From]=i.State) AND
                                           (t1.[Incident] = i.ObjectID))				
                                         ]]>
              </ds:param-selector>
						</ds:switch>
					</ds:param>
					<ds:param n="Folders" array="1" vt="uuid">
						<ds:array-param-processor macro="#FolderIDs#" />
					</ds:param>
					<ds:param n="Initiators" array="1" vt="uuid">
						<ds:param-selector op="in">e.ObjectID</ds:param-selector>
					</ds:param>
					<ds:param n="Participants" array="1" vt="uuid">
						<ds:array-param-processor macro="#Participants#" />
					</ds:param>
					<ds:param n="InputDateBegin" vt="date">
						<ds:param-selector op="not-is-null"><![CDATA[ i.InputDate >= @InputDateBegin ]]></ds:param-selector>
					</ds:param>
					<ds:param n="InputDateEnd" vt="date">
						<ds:param-selector op="not-is-null"><![CDATA[ i.InputDate < @InputDateEnd + 1 ]]></ds:param-selector>
					</ds:param>
					<ds:param n="LastActivityDateBegin" vt="date">
						<ds:param-selector op="not-is-null">
              <![CDATA[ 
					 (SELECT MAX(tmp.d)
						FROM 
							(
								SELECT /*дата заведения инцидента*/
									i2.InputDate d
								FROM dbo.Incident i2 WITH (NOLOCK) 
								WHERE i2.ObjectID = i.ObjectID 
								UNION 
								SELECT /*дата последнего списания времени по заданиям инцидента*/
									IsNull(Max(ts2.RegDate), 0) d
								FROM dbo.TimeSpent ts2 WITH (NOLOCK) 
									JOIN dbo.Task t2 WITH (NOLOCK) ON ts2.Task = t2.ObjectID
								WHERE t2.Incident = i.ObjectID 
								UNION 
								SELECT /*дата последнего изменения состояния инцидента*/
									IsNull(Max(h2.ChangeDate), 0) d  
								FROM dbo.IncidentStateHistory h2 WITH (NOLOCK) 
								WHERE h2.Incident = i.ObjectID 
							) tmp						
						) >= @LastActivityDateBegin 
					]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="LastActivityDateBeginWithoutTasks" vt="date">
						<ds:param-selector op="not-is-null">
              <![CDATA[ 
					 (SELECT MAX(tmp.d)
						FROM 
							(
								SELECT /*дата заведения инцидента*/
									i2.InputDate d
								FROM dbo.Incident i2 WITH (NOLOCK) 
								WHERE i2.ObjectID = i.ObjectID 
								UNION 
								SELECT /*дата последнего изменения состояния инцидента*/
									IsNull(Max(h2.ChangeDate), 0) d  
								FROM dbo.IncidentStateHistory h2 WITH (NOLOCK) 
								WHERE h2.Incident = i.ObjectID 
							) tmp						
						) >= @LastActivityDateBeginWithoutTasks 
					]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="LastActivityDateEnd" vt="date">
						<ds:param-selector op="not-is-null">
              <![CDATA[ 
						(SELECT MAX(tmp.d)
						FROM 
							(
								SELECT /*дата заведения инцидента*/
									i2.InputDate d
								FROM dbo.Incident i2 WITH (NOLOCK) 
								WHERE i2.ObjectID = i.ObjectID 
								UNION 
 								SELECT /*дата последнего списания времени по заданиям инцидента*/
									IsNull(Max(ts2.RegDate), 0) d
								FROM dbo.TimeSpent ts2 WITH (NOLOCK) 
									JOIN dbo.Task t2 WITH (NOLOCK) ON ts2.Task = t2.ObjectID
								WHERE t2.Incident = i.ObjectID 
								UNION 
								SELECT /*дата последнего изменения состояния инцидента*/
									IsNull(Max(h2.ChangeDate), 0) d  
								FROM dbo.IncidentStateHistory h2 WITH (NOLOCK) 
								WHERE h2.Incident = i.ObjectID 
							) tmp						
						) < @LastActivityDateEnd + 1
						 ]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="LastActivityDateEndWithoutTasks" vt="date">
						<ds:param-selector op="not-is-null">
              <![CDATA[ 
						(SELECT MAX(tmp.d)
						FROM 
							(
								SELECT /*дата заведения инцидента*/
									i2.InputDate d
								FROM dbo.Incident i2 WITH (NOLOCK) 
								WHERE i2.ObjectID = i.ObjectID 
								UNION 
								SELECT /*дата последнего изменения состояния инцидента*/
									IsNull(Max(h2.ChangeDate), 0) d  
								FROM dbo.IncidentStateHistory h2 WITH (NOLOCK) 
								WHERE h2.Incident = i.ObjectID 
							) tmp						
						) < @LastActivityDateEndWithoutTasks + 1
						 ]]>
            </ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
