			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
		  		SELECT 
					tl.ObjectID,
					c.Name AS CauseName,  
					c.ObjectID AS CauseGuid,
					tl.LossFixed,
					tl.LostTime AS LostTime,
					emp.FirstName + ' ' + emp.LastName AS SystemUser,
