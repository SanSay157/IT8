		<i:objects-list t="Интеграция с Microsoft Project: список проектов" n="MsProjectIntegration_Projects" maxrows="1000000" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
				SELECT
					f.ObjectID,
					f.Name,
					itype.Name,
					f.Parent,
					CASE 
						WHEN @LoadFullPath = 1 THEN dbo.GetFolderPath(f.ObjectID, 1) 
						ELSE ''
					END AS FullName,
					f.ActivityType,
					f.Customer
				FROM
					dbo.[Folder] f WITH (NOLOCK) 
						LEFT JOIN dbo.[IncidentType] itype WITH (NOLOCK) ON f.[DefaultIncidentType] = itype.[ObjectID]
				WHERE_CLAUSE
				ORDER BY
					f.[Name]
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="FolderID" vt="uuid">
						<ds:param-selector op="not-is-null"><![CDATA[ f.ObjectID = :FolderID ]]></ds:param-selector>
					</ds:param>
					<ds:param n="ParentID" vt="uuid">
						<ds:param-selector op="not-is-null"><![CDATA[ f.Parent = :ParentID ]]></ds:param-selector>
					</ds:param>
					<ds:param n="Empty" vt="i2">
						<ds:param-selector op="equal" value="1"><![CDATA[ f.ObjectID IS NULL ]]></ds:param-selector>
