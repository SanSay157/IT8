		<i:objects-list t="Список активностей" n="ProjectsList" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0" off-load="1">
			<i:client-script>s-list-FolderList.vbs</i:client-script>
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
					SELECT TOP @@MAXROWS 
						f.ObjectID,
						f.[Name] AS FolderName,
						o.[Name] AS CustomerName,
						CASE f.Type
							WHEN 1 THEN 'Проект'
							WHEN 4 THEN 'Подготовка тендера'
							WHEN 8 THEN 'Пресейл'
							WHEN 16 THEN 'Папка'
							ELSE ''
						END AS FolderTypeName,
						dbo.NameOf_FolderStates( f.State ) AS FolderStateName,
						emp.LastName + ' ' + emp.FirstName AS CustomerDirector,
						f.ExternalID AS NavisionID,
						dbo.GetFolderPath(f.ObjectID, 1),
						dbo.GetFolderIconSelector(f.Type, f.State, NULL, NULL) AS ICON_SELECTOR
					FROM
						dbo.Folder f WITH (NOLOCK)
						JOIN dbo.Organization o WITH (NOLOCK) ON f.Customer = o.ObjectID
						LEFT JOIN dbo.Employee emp WITH (NOLOCK) ON emp.ObjectID = o.Director
					WHERE_CLAUSE
					ORDER BY o.Name, f.Name
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="FolderName" vt="string" use-wildcard="implicit-on-end">
						<ds:param-selector op="not-is-null">f.Name LIKE :FolderName</ds:param-selector>
					</ds:param>
					<ds:param n="CustomerName" vt="string" use-wildcard="implicit-on-end">
						<ds:param-selector op="not-is-null">o.Name LIKE :CustomerName</ds:param-selector>
					</ds:param>
					<ds:param n="OnlyRootFolder" vt="boolean">
						<ds:param-selector op="not-equal" value="0">f.Parent IS NULL</ds:param-selector>
					</ds:param>
					<ds:param n="Directions" array="1" vt="uuid">
						<ds:array-param-processor values-macro="#ID_LIST#">
              <![CDATA[
							EXISTS (
								SELECT 1 FROM dbo.FolderDirection fd WITH(NOLOCK)
								WHERE fd.Folder = f.ObjectID AND fd.Direction IN (#ID_LIST#)
							)
						]]>
            </ds:array-param-processor>
					</ds:param>
					<ds:param n="FolderTypes" vt="i4">
						<ds:param-selector op="not-equal" value="0"><![CDATA[ f.Type & :FolderTypes != 0 ]]></ds:param-selector>
					</ds:param>
					<ds:param n="FolderState" vt="i4">
						<ds:param-selector op="not-equal" value="0"><![CDATA[ f.State & :FolderState != 0 ]]></ds:param-selector>
					</ds:param>
					<ds:param n="NavCode" vt="string" use-wildcard="implicit-on-end">
						<ds:param-selector op="not-is-null">f.ExternalID LIKE :NavCode</ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
			<i:filter-as-editor ot="FilterFolderList" metaname="Editor" height="150" />
			<i:list-menu>
				<i:menu xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
