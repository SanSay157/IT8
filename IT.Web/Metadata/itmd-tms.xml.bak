		<i:objects-list n="TendersList" t="Тендеры" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<i:client-script>s-TendersList.vbs</i:client-script>
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
				SELECT TOP @@MAXROWS 
					t.ObjectID,
					t.Name,
          t.DocFeedingDate,
          t.DateTorg1,
          t.DateTorg2,
          dbo.NameOf_LotState(l.State),
					ISNULL(c.ShortName, c.Name),
          ISNULL(o.ShortName, o.Name),
          dbo.GetTenderExecutorsFromMarketing(t.ObjectID),
          [dbo].[GetTenderManagers](t.ObjectID),			
					dbo.GetLotTenderIconSelector(l.State, t.DocFeedingDate, getdate()) AS ICON_SELECTOR
				FROM dbo.Tender t
					INNER JOIN dbo.Lot AS l WITH(NOLOCK) ON l.Tender = t.ObjectID
          LEFT JOIN dbo.Organization AS c WITH(NOLOCK) ON c.ObjectID = t.TenderCustomer
					LEFT JOIN dbo.Organization AS o WITH(NOLOCK) ON o.ObjectID = t.Organizer
				WHERE
					(	
						( :Company IS NULL ) OR
						( :Company IS NOT NULL AND EXISTS (
								SELECT 1 
								FROM dbo.Lot l WITH(NOLOCK) JOIN dbo.LotParticipant lP WITH(NOLOCK) ON lP.Lot = l.ObjectID
								WHERE lP.ParticipantOrganization = :Company AND l.Tender = t.ObjectID )
						) 
					) 
					AND SEARCH_CONDITIONS
          ORDER BY t.DocFeedingDate DESC
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="CustomerName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							( c.ShortName LIKE :CustomerName OR c.Name LIKE :CustomerName )
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="OrganizerName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							ISNULL(o.ShortName, o.Name) LIKE :OrganizerName 
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="TenderName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							t.Name LIKE :TenderName
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="TenderState" vt="i2">
						<ds:param-selector op="not-is-null"><![CDATA[ l.State = :TenderState ]]></ds:param-selector>
					</ds:param>
					<ds:param n="DocFeedingBegin" vt="dateTime">
						<ds:param-selector op="not-is-null">
              <![CDATA[ 
							t.DocFeedingDate >= dbo.DATETRIM(:DocFeedingBegin) 
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="DocFeedingEnd" vt="dateTime">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							t.DocFeedingDate < DATEADD(d, 1, dbo.DATETRIM(:DocFeedingEnd))
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="Company" vt="uuid" />
					<ds:param n="Competitor" vt="uuid">
						<ds:param-selector op="not-is-null">
              <![CDATA[
						  EXISTS
						  (
							  SELECT 1
							  FROM dbo.Lot AS l WITH(NOLOCK)
								  INNER JOIN dbo.LotParticipant AS p WITH(NOLOCK) ON p.Lot = l.ObjectID
							  WHERE l.Tender = t.ObjectID
								  AND p.ParticipationType <> 1
								  AND p.ParticipantOrganization = :Competitor
						  )
						]]>
            </ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
