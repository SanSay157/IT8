		<i:objects-list n="LotsList" t="Лоты" off-rownumbers="1" xmlns:i="http://www.croc.ru/Schemas/XmlFramework/Interface/1.0">
			<i:client-script>s-LotsList.vbs</i:client-script>
			<ds:data-source xmlns:ds="http://www.croc.ru/Schemas/XmlFramework/Data/1.0">
				<ds:cmd-text>
          <![CDATA[
				SELECT TOP @@MAXROWS 
          L.ObjectID,
          L.Name,
 					T.DocFeedingDate,
          T.DateTorg1,
          T.DateTorg2,
          dbo.NameOf_LotState( #LotStateExpr# ),
          ISNULL(O.ShortName, O.Name),
          ISNULL(Org.ShortName, Org.Name),
          dbo.GetTenderExecutorsFromMarketing(T.ObjectID),
					dbo.GetLotTenderIconSelector( #LotStateExpr#, T.DocFeedingDate, GETDATE() ) AS ICON_SELECTOR
				FROM 
					dbo.Lot AS L WITH(NOLOCK)
					INNER JOIN dbo.Tender AS T WITH(NOLOCK) ON T.ObjectID = L.Tender
					LEFT JOIN dbo.Organization AS O WITH(NOLOCK) ON O.ObjectID = T.TenderCustomer
          LEFT JOIN dbo.Organization AS Org WITH(NOLOCK) ON Org.ObjectID = T.Organizer
					LEFT JOIN (
						/* Подзапрос на проверку итогового состояния "своей" 
							компании как выигравшей конкурс по данному лоту: */
						SELECT lP.Lot, lP.Winner
						FROM dbo.LotParticipant lP WITH(NOLOCK)
						WHERE 
							( @Company IS NULL AND lP.ParticipantOrganization IS NULL ) OR
							( @Company IS NOT NULL AND lP.ParticipantOrganization = @Company )
					) lP ON lP.Lot = L.ObjectID
				WHERE
					(	
						( :Company IS NULL ) OR
						( :Company IS NOT NULL AND EXISTS (
								SELECT 1
								FROM dbo.LotParticipant AS lP WITH(NOLOCK)
								WHERE lP.Lot = L.ObjectID AND lP.ParticipantOrganization = :Company )
						) 
					) 
					AND SEARCH_CONDITIONS
				]]>
        </ds:cmd-text>
				<ds:params>
					<ds:param n="CustomerName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							(O.ShortName LIKE :CustomerName OR O.Name LIKE :CustomerName)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="OrganizerName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							(Org.ShortName LIKE :OrganizerName OR Org.Name LIKE :OrganizerName)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="LotName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							(L.Name LIKE :LotName)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="TenderName" vt="string" use-wildcard="conditional-on-end">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							T.Name LIKE :TenderName 
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="State" vt="i2">
						<ds:param-selector op="not-is-null"><![CDATA[ #LotStateExpr# = :State ]]></ds:param-selector>
					</ds:param>
					<ds:param n="DocFeedingBegin" vt="dateTime">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							T.DocFeedingDate >= dbo.DATETRIM(:DocFeedingBegin)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="DocFeedingEnd" vt="dateTime">
						<ds:param-selector op="not-is-null">
              <![CDATA[
							T.DocFeedingDate < DATEADD(d, 1, dbo.DATETRIM(:DocFeedingEnd))
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="Director" vt="uuid">
						<ds:param-selector op="not-is-null"><![CDATA[ T.Director = :Director ]]></ds:param-selector>
					</ds:param>
					<!-- 
						Условие на компанию определено непосредственно в запросе; 
						здесь НЕ ИСПОЛЬЗУЕТСЯ param-selector, т.к. идентификатор компании
						используется в запросе помимо WHERE-условия, как операнд функции
						GetTenderState.	-->
					<ds:param n="Company" vt="uuid" />
					<!-- 
						На основании значения параметра формируется макроподстановка
						с выражением, определяющим способ получения состояния лота.
						NB! Сформированная макроподстановка используется также в условии 
						параметра State! -->
					<ds:param n="IsStrictStateCalc" vt="boolean" required="1">
						<ds:switch macro="#LotStateExpr#" explicit="1">
							<ds:param-selector op="equal" value="0">
                <![CDATA[
								L.State
							]]>
              </ds:param-selector>
							<ds:param-selector op="not-equal" value="0">
                <![CDATA[
								(CASE 
									/* Если "нашей" в конкурсе нет - берем состояние конкурсе */
									WHEN lP.Lot IS NULL THEN L.State 
									ELSE 
										CASE 
											/* Иначе - если состояние лота "Выигран" или "Проигран", то 
												проверяем итоговый статус "нашей" компании: */
											WHEN L.State IN (5, 6) THEN 
												CASE 
													/* Если "наша" компания - победитель, то состяние - "Выигран" (5) */
													WHEN lP.Winner<>0 THEN 5
													/* иначе - "Проигран" (6) */
													ELSE 6 
												END
											/* Иначе - берем состояние конкурса */
											ELSE L.State 
										END
								END )
							]]>
              </ds:param-selector>
						</ds:switch>
					</ds:param>
					<!-- -->
					<ds:param n="CustomerBranch" vt="uuid">
						<ds:param-selector op="not-is-null">
              <![CDATA[
						EXISTS
						(
							SELECT 1
							FROM dbo.Organization_Branch AS OB WITH(NOLOCK)
							WHERE OB.ObjectID = O.ObjectID AND OB.Value = :CustomerBranch
						)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="DocumentationType" vt="i2">
						<ds:param-selector op="equal" value="1">
              <![CDATA[ 
							(S2.SumValue > 0)
						]]>
            </ds:param-selector>
						<ds:param-selector op="equal" value="2">
              <![CDATA[ 
							(S2.SumValue = 0 OR S2.SumValue IS NULL)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="LotManager" vt="uuid">
						<ds:param-selector op="not-is-null"><![CDATA[L.Manager = :LotManager]]></ds:param-selector>
					</ds:param>
					<ds:param n="NoLotManager" vt="boolean">
						<ds:param-selector op="equal" value="1"><![CDATA[L.Manager IS NULL]]></ds:param-selector>
					</ds:param>
					<ds:param n="GuaranteeType" vt="i2">
						<ds:param-selector op="equal" value="1">
              <![CDATA[
							(S.SumValue > 0 OR G.PortionValue IS NOT NULL)
						]]>
            </ds:param-selector>
						<ds:param-selector op="equal" value="2">
              <![CDATA[
							( (S.SumValue = 0 OR S.SumValue IS NULL) AND G.PortionValue IS NULL )
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="Competitor" vt="uuid">
						<ds:param-selector op="not-is-null">
              <![CDATA[
						EXISTS
						(
							SELECT 1
							FROM dbo.LotParticipant AS P WITH(NOLOCK)
							WHERE P.Lot = L.ObjectID
								AND P.ParticipationType <> 1
								AND P.ParticipantOrganization = :Competitor
						)
						]]>
            </ds:param-selector>
					</ds:param>
					<ds:param n="CompetitorType" vt="i2">
						<ds:param-selector op="not-is-null">
              <![CDATA[
						EXISTS
						(
							SELECT 1
							FROM dbo.LotParticipant AS P WITH(NOLOCK)
							WHERE P.Lot = L.ObjectID AND P.ParticipationType = :CompetitorType
						)
						]]>
            </ds:param-selector>
					</ds:param>
				</ds:params>
			</ds:data-source>
