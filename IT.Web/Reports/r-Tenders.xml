	<r:params>
		<r:param r:n="IntervalBegin" r:vt="date" />
		<r:param r:n="IntervalEnd" r:vt="date" />
		<r:param r:n="Company" r:vt="uuid" />
		<r:param r:n="TenderCustomer" r:vt="uuid" />
		<r:param r:n="Organizer" r:vt="uuid" />
